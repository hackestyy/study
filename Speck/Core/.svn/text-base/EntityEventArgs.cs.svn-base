using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ZteApp.ProductService.Core
{
    public class EntityEventArgs
    {
        private Component mEntity;

        public EntityEventArgs(Component entity)
        {
            mEntity = entity;    
        }

        public Component Entity
        {
            get
            {
                return mEntity;
            }
            set
            {
                mEntity = value;
            }
        }
    }
}
